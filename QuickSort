class QuickSort{
    public static <T extends Comparable<T>> void Sort(T[] arr, int low, int high)
    {
        if (low < high)
        {
            int pi = partition(arr, low, high); // Index of pivot
            // Sort elements before pivot and after pivot
            Sort(arr, low, pi - 1);
            Sort(arr, pi + 1, high);
        }
    }

    /**
     * Swaps 2 elements with indexes 'from' and 'to' in array 'arr'
     * @param arr Array itself
     * @param from Index of the first element
     * @param to Index of the second element
     */
    public static <T extends Comparable<T>> void swap(T[] arr, int from, int to)
    {
        T temp = arr[from];
        arr[from] = arr[to];
        arr[to] = temp;
    }

    /**
     * Make a partition of input array in certain interval from low to high-1
     * @param arr Array to make partition
     * @param low Index of first element in interval
     * @param high Index of last element in interval plus one
     * @param <T> Type of elements in array. They must extend Comparable interface
     * @return index of last element in input array in new  one
     */
    public static <T extends Comparable<T>> int partition(T[] arr, int low, int high)
    {
        T pivot = arr[high];
        int i = (low - 1);

        for(int j = low; j <= high - 1; j++)
        {
            if (arr[j].compareTo(pivot) <=0)
            {
                i++;
                swap(arr, i, j);
            }
        }
        swap(arr, i + 1, high);
        return (i + 1);
    }
}
